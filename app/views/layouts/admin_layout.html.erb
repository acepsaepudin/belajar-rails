    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">

    <title>DASHGUM - FREE Bootstrap Admin Template</title>

    <!-- Bootstrap core CSS -->
    <%= stylesheet_link_tag 'bootstrap', media: 'all', 'data-turbolinks-track' => true %>
    <!--external css-->
    <%= stylesheet_link_tag 'font-awesome/css/font-awesome', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag 'zabuto_calendar.css', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag 'jquery.gritter.css', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag 'lineicons/style.css', media: 'all', 'data-turbolinks-track' => true %>

    <!-- Custom styles for this template -->
    <%= stylesheet_link_tag 'style.css', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag 'style-responsive.css', media: 'all', 'data-turbolinks-track' => true %>
    <style><%= yield :stylesheets %></style>
    <%= javascript_include_tag 'chart-master/Chart.js', 'data-turbolinks-track' => true %>

