<H1>ini index untuk tags</H1>
<div class="container">
<table class="table table-striped">
 <tr>
	<th>Name</th>
	<th>Action</th>
 </tr>
 <% @tags.each do |tag| %>
 <tr>
	<td><%= tag.name %></td>
	<td><%= link_to "Show", tag_path(tag), class: 'btn btn-info' %>
	<%= link_to "Edit", edit_tag_path(tag), class: 'btn btn-success' %>
	<%= link_to 'Delete', tag_path(tag),method: :delete , class: 'btn btn-danger'%>
 </tr>
 <% end %>
</table>
</div>
